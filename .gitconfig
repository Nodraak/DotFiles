[user]
    name = Adrien Chardon
    email = adrienchardon@mailoo.org
[alias]
    s = status
    c = commit
    cm = commit -m
    co = checkout
    psho = push origin
    pshfo = push -f origin
    pllo = pull origin
    pllfo = pull -f origin
    logo = log --pretty=oneline --abbrev-commit --color
    pulledcode = log -p --reverse --no-merges --stat @{1}..
    pulledcommit = log --reverse --no-merges --stat @{1}..
    ri = rebase -i
[color]
    ui = auto
[push]
	default = matching
[init]
	templatedir = ~/.git_template
